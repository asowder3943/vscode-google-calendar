{
  "name": "google-calendar-inegration",
  "displayName": "VSCode Google Calendar ",
  "description": "It allows to you to sync your google calendar with VS Code",
  "publisher": "RigRex",
  "icon": "images/icon.png",
  "repository": {
    "type": ".git",
    "url": "https://github.com/engrmafzaalch/vscode-google-calendar.git"
  },
  "version": "0.1.2",
  "engines": {
    "vscode": "^1.56.0"
  },
  "keywords": [
    "google",
    "calendar",
    "Google Calendar VS Code",
    "google calendar",
    "vscode calendar"
  ],
  "categories": [
    "Other",
    "Visualization",
    "Notebooks",
    "Education"
  ],
  "activationEvents": [
    "*"
  ],
  "galleryBanner": {
    "color": "#3262a9",
    "theme": "dark"
  },
  "main": "./extension.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.56.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "contributes": {
    "views": {
      "events-panel": [
        {
          "id": "upcoming-events",
          "name": "Upcoming Events"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "upcoming-events.refreshEntry",
          "when": "view == upcoming-events",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "upcoming-events.openEntry",
          "when": "view == upcoming-events && viewItem == event",
          "group": "inline"
        }
      ]
    },
    "commands": [
      {
        "command": "upcoming-events.refreshEntry",
        "title": "Refresh Calendar",
        "icon": {
          "light": "images/light/sync.svg",
          "dark": "images/dark/sync.svg"
        }
      },
      {
        "command": "upcoming-events.openEntry",
        "title": "Open",
        "icon": {
          "light": "images/light/open.svg",
          "dark": "images/dark/open.svg"
        }
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "events-panel",
          "title": "Google Calendar",
          "icon": "images/light/calendar.svg"
        }
      ]
    }
  },
  "dependencies": {
    "axios": "^0.28.0",
    "polka": "^0.5.2"
  }
}
